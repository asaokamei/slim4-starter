{
    "name": "asaokamei/slime-php",
    "description": "my starter project for Slim4 + PHP-DI .",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "asaokamei",
            "email": "asao.kamei@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "nyholm/psr7": "^1.3",
        "nyholm/psr7-server": "^1.0.0",
        "slim/http": "^1.0",
        "slim/csrf": "^1.0",
        "slim/slim": "^4.0",
        "monolog/monolog": "^2.0",
        "php-di/php-di": "^6.1",
        "vlucas/phpdotenv": "^5.2",
        "slim/twig-view": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}
